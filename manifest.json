{
  "manifest_version": 3,
  "name": "Request Logger",
  "version": "1.5",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "./js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "./html/popup.html"
  }
}
